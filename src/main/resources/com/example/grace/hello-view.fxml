<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.media.*?>
<?import javafx.scene.text.*?>

<VBox fx:id="vBox" alignment="CENTER" minHeight="650.0" minWidth="700.0" prefHeight="656.0" prefWidth="734.0" spacing="20.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.grace.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <children>
      <MediaView fx:id="mVideo" fitHeight="400.0" fitWidth="700.0" />
      <HBox fx:id="hBox" alignment="CENTER_LEFT" prefHeight="51.0" prefWidth="694.0" style="-fx-background-color: #000000; -fx-border-radius: 8;">
         <children>
            <Button fx:id="play" mnemonicParsing="false" style="-fx-cursor: hand;" text="Play">
               <HBox.margin>
                  <Insets left="225.0" />
               </HBox.margin>
            </Button>
            <Button fx:id="pause" mnemonicParsing="false" style="-fx-cursor: hand;" text="Pause">
               <HBox.margin>
                  <Insets left="15.0" />
               </HBox.margin>
            </Button>
            <Button fx:id="stop" mnemonicParsing="false" style="-fx-cursor: hand;" text="Stop">
               <HBox.margin>
                  <Insets left="15.0" />
               </HBox.margin>
            </Button>
         </children>
         <VBox.margin>
            <Insets top="20.0" />
         </VBox.margin>
         <padding>
            <Insets bottom="1.0" />
         </padding>
      </HBox>
      <HBox fx:id="volume" alignment="CENTER_LEFT" prefHeight="50.0" prefWidth="694.0">
         <children>
            <Label fx:id="currentDur" style="-fx-cursor: hand;">
               <font>
                  <Font name="JetBrains Mono Bold" size="18.0" />
               </font>
               <HBox.margin>
                  <Insets left="90.0" top="5.0" />
               </HBox.margin>
               <padding>
                  <Insets left="10.0" right="10.0" />
               </padding>
            </Label>
            <Slider fx:id="slider" max="125.0" prefHeight="24.0" prefWidth="610.0" style="-fx-cursor: hand;">
               <HBox.margin>
                  <Insets left="62.0" top="1.0" />
               </HBox.margin>
               <padding>
                  <Insets bottom="5.0" left="10.0" right="10.0" top="5.0" />
               </padding>
            </Slider>
            <Slider fx:id="slidervolume" max="1.0" style="-fx-cursor: hand;">
               <HBox.margin>
                  <Insets left="60.0" />
               </HBox.margin>
            </Slider>
         </children>
         <padding>
            <Insets bottom="10.0" />
         </padding>
         <VBox.margin>
            <Insets />
         </VBox.margin>
      </HBox>
   </children>
</VBox>
